      grecaptcha.execute('6LfgQZcUAAAAANr8X08XXmnVGVc3SJ_V7Owo49h2', {action: 'homepage'}).then(function(token) {
         var recaptchaResponse = document.getElementById('recaptchaResponse');
		 gath1t=token;
                recaptchaResponse.value = gath1t;
      });
  });
</script>



