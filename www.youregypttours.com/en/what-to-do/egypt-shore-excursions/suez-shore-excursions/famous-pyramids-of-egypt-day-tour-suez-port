	<script>
	$(document).ready(function(){
	$("#submit_inq").click(function(){
		event.preventDefault();

		if($('#fname_input').val()=='' || $('#lname_input').val()=='' || $('#email_input').val()=='' || $('#phone_input').val()=='' ||
		$('#adress_input').val()=='' || $('#no-adult').val()=='' || $('#inq_input').val()==''){


			if ( $( "#req_hint" ).length ) {



			}else{
				$( "<p id='req_hint' style='text-align:center;color:red'>Please fill all required fields!</p>" ).insertAfter( "#submit_inq" );
				}


		}else{
			$("#inq_form").submit();
		}





	});

	});


	</script>


